{
  "private": true,
  "name": "pointbrowser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s pdev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "nf start",
    "pdev": "preact watch",
    "lint": "eslint src",
    "electron": "electron .",
    "ebuild": "npm run build && node_modules/.bin/build"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "no-unused-vars": "warn",
      "react/sort-comp": "off",
      "lines-around-comment": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.11.0",
    "preact-cli": "^2.0.0",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "encoding": "^0.1.12",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.15",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.4.3",
    "preact-router": "^2.5.5"
  },
  "description": "Demo App",
  "homepage": "./",
  "main": "electron/main.js",
  "build": {
    "productName": "Demo App",
    "appId": "com",
    "electronVersion": "2.0.2",
    "files": [
      "build/**/*",
      "electron/*"
    ]
  }
}
